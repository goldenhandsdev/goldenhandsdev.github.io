{
    "_version": "1.58.0",
    "sap.app": {
        "id": "projectlist",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}"
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "models": {
            "i18n": {
                "dataSource": "i18n",
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "projectlist.i18n.i18n"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "projectlist.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "appHome",
                    "pattern": "",
                    "target": ["master"]
                },
                {
                    "name": "master",
                    "pattern": "master",
                    "target": ["master"]
                },
                {
                    "name": "detail",
                    "pattern": "project/{projectId}",
                    "target": ["master", "detail"]
                }
            ],
            "targets": {
                "master": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "master",
                    "viewName": "Master",
                    "controlAggregation": "pages",
                    "controlId": "app"
                },
                "detail": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "detail",
                    "viewName": "Detail",
                    "controlAggregation": "pages",
                    "controlId": "app"
                }
            }
        }
    },
    "sap.platform.hcp": {
        "uri": "webapp",
        "_version": "1.1.0"
    },
    "sap.app/dataSources": {
        "i18n": {
            "uri": "i18n/i18n.properties",
            "type": "Properties"
        }
    }
}
